pipeline {
    agent any
    stages {
        stage('Build and run docker image') {
            steps {
                sh 'sudo docker pull realsahilthakur/2244_ica2:latest'
                sh 'sudo docker run -d -p 8082:80 realsahilthakur/2244_ica2:latest'
            } 
        }
        stage('testing') {
            steps {
                sh 'curl -I 192.168.196.139:8080'
            }
        }
    }
}
